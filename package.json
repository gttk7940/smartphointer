{
  "name": "smartphointer",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "homepage": "https://starmine7940.github.io/smartphointer/",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "rm": "rm -rf docs",
    "mv": "cp -R dist docs",
    "git": "git add . && git commit -m 'deploy' && git push origin main",
    "deploy": "npm run rm && npm run build && npm run mv && npm run git"
  },
  "dependencies": {
    "react": "^19.2.3",
    "react-device-detect": "^2.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.3.0"
  }
}
